    "@types/eslint": "^9.6.1",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.13.1",
    "globals": "^16.5.0",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
