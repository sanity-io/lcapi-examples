    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.20.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/postcss": "^4.0.14",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.22.0",
    "eslint-plugin-svelte": "^3.3.2",
    "globals": "^16.0.0",
    "svelte": "^5.23.1",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.0.14",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2"
