    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.17.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/postcss": "^4.0.7",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.20.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.15.0",
    "svelte": "^5.20.2",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^4.0.7",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.0"
  }
