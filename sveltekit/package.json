    "lint": "eslint --cache .",
    "preview": "vite preview",
    "typegen:generate": "sanity typegen generate"
  },
  "dependencies": {
    "@sanity/client": "^6.28.0",
    "groq": "^3.75.0"
  },
  "devDependencies": {
