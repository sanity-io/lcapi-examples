    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.24.0",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "svelte": "^5.25.10",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.1.3",
